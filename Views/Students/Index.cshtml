@{
    ViewBag.Title = "Students";
}
<h2>Students</h2>

<table class="table">
    <thead>
        <tr>
            <th>StudentId</th>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Email</th>
            <th>Phone Number</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var student in Model)
        {
            <tr>
                <td>@student.StudentId</td>
                <td>@student.FirstName</td>
                <td>@student.LastName</td>
                <td>@student.Email</td>
                <td>@student.PhoneNumber</td>
            </tr>
        }
    </tbody>
</table>
